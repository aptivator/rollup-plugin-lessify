    "rollup-pluginutils": "^1.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aptivator/rollup-plugin-lessify"
  },
  "bugs": {
    "url": "https://github.com/aptivator/rollup-plugin-lessify/issues"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "buble": "^0.15.2",
    "eslint": "^3.2.2",
    "express": "^4.15.2",
    "mocha": "^3.0.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-node-resolve": "^2.0.0"
  }
