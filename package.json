  "description": "A rollup plugin that compiles less files into css",
  "main": "dist/lessify.js",
  "jsnext:main": "dist/lessify.es.js",
  "scripts": {
    "test": "mocha test/*.js",
    "pretest": "npm run build",
    "build": "rollup -c",
